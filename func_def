def get_from_repository(R = "", func_defi = ""):
    start = R.find(func_defi)
    end = R[start+1:].find("\n\n")+1+start
    return (R[start:end])


def get_repository(R = ""):
    ret = []
    while "\n\n" in R:
        ret.append(R[R.find("def "):R[R.find("\n\n")])
        R = R[R.find("\n\n")+1:]
    return ret


def hello():
    print("Hello World")


def how():
    print("How are you?")


def some_def():
    print("-"*30)
    print("Some random function")
    print("-"*30)

