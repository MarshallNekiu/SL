def get_repository(R = ""):
    ret = []
    while "def " in R:
        ret.append(R[R.find("def "):R[R.find("def ")+1].find("def ")])
        R = R[R.find("def ")+1:]
    return ret


def get_from_repository(R = "", func_defi = ""):
    start = R.find(func_defi)
    end = R[start+1:].find("def ")+start
    return (R[start:end])


def hello():
    print("Hello World")


def how():
    print("How are you?")


def some_def():
    print("-"*30)
    print("Some random function")
    print("-"*30)
