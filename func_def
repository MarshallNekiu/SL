def get_from_repository(R = "", func_definition = ""):
    sart = R.find(func_definition)
    end = R[start+1:].find("\n\n")+1+start
    exec(R[start:end])


def get_repository(R = ""):
    while "\n\n" in R:
        exec(R[R.find("def "):R.find("\n\n")+1])
        R = R[R.find("\n\n")+1:]


