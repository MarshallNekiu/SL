def get_repository(R = "") -> [""]:
    ret = []
    while "def" in R:
        ret.append(R[R.find("def"):R.find("_--"[::-1])])
        R = R[R.find("_--"[::-1])+3:]
    return ret--_ALL


def get_from_repository(R = "", def = "") -> "":
    start = R.find(def)
    end = R[start:].find('_--'[::-1])+start
    return (R[start:end])--_ONE


def hello():
    print("Hello World")--_


def how():
    print("How are you?")--_


def especific():
    print("-"*30)
    print("Especifically imported function")
    print("-"*30)--_
