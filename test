def get_repository(repository):
    ret = []
    while "def" in repository:
        ret.append(repository[repository.find("def"):repository.find("_--"[::-1])])
        repository = repository[repository.find("_--"[::-1])+3:]
    return ret--_ALL


def get_from_repository(repository, func):
    start = repository.find(func)
    end = repository[start:].find('_--'[::-1])+start
    return (repository[start:end])--_ONE


def hello():
    print("Hello World")--_


def how():
    print("How are you?")--_


def especific():
    print("-"*30)
    print("Especifically imported function")
    print("-"*30)--_


def get_repository2(repository):
    while "def" in repository:
        yield(repository[repository.find("def"):repository.find("_--"[::-1])])
        repository = repository[repository.find("_--"[::-1])+3:]--_ALL

